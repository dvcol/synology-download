{
  "name": "@dvcol/synology-extension",
  "version": "3.0.3",
  "packageManager": "yarn@3.5.0",
  "description": "A basic synology download station client chrome extension",
  "keywords": [
    "extension",
    "webex",
    "web-extension",
    "synology"
  ],
  "license": "MIT",
  "author": "dvcol",
  "homepage": "https://github.com/dvcol/synology-download",
  "bugs": "https://github.com/dvcol/synology-download/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcol/synology-download.git"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/types/pages/web/index.d.ts",
  "files": [
    "*.md",
    "LICENSE",
    "dist/src",
    "dist/_locales",
    "dist/types/pages/web/modules",
    "dist/types/pages/web/index.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build:web": "node webpack/build/web.js",
    "build:web:analyse": "ANALYSE_BUNDLE=true yarn build:web",
    "build:extension": "node webpack/build/extension.js",
    "build:extension:analyse": "ANALYSE_BUNDLE=true yarn build:extension",
    "build": "yarn build:web && yarn build:extension",
    "build:debug": "DEBUG=true DEVTOOL=true yarn build",
    "start:web": "node webpack/dev/web.js",
    "start:extension": "node webpack/dev/extension.js",
    "devtools": "redux-devtools --hostname=localhost --port=8000",
    "test:unit": "jest --watchAll=false --coverage ",
    "test:change": "yarn test:unit -- --onlyChanged",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "style": "stylelint src/**/*.{css,scss,less,html}",
    "style:fix": "yarn style --fix",
    "version": "standard-version --skip.changelog --skip.commit --skip.tag",
    "release": "git tag -d $(git tag -l \"v*-rc\") && standard-version --skip.bump",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "engines": {
    "yarn": ">=3.2.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@dvcol/web-extension-utils": "~2.1.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@hot-loader/react-dom": "^17.0.2",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.69",
    "@mui/material": "^5.5.0",
    "@mui/system": "^5.11.15",
    "@mui/x-data-grid": "^6.0.3",
    "@reduxjs/toolkit": "^1.7.2",
    "global": "^4.4.0",
    "notistack": "^2.0.3",
    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-router-hash-link": "^2.4.3",
    "redux": "^4.1.2",
    "rxjs": "^7.8.0",
    "uuid": "^8.3.2",
    "webext-redux": "^2.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@dvcol/eslint-plugin-presets": "1.2.1",
    "@dvcol/stylelint-plugin-presets": "^1.1.2",
    "@redux-devtools/cli": "^2.0.0",
    "@redux-devtools/extension": "^3.2.5",
    "@redux-devtools/remote": "^0.8.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/chrome": "^0.0.224",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@types/testing-library__react": "^10.2.0",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "chrome-webstore-upload-cli": "^2.0.1",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.36.0",
    "extract-changelog-release": "^1.0.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^13.1.2",
    "merge-jsons-webpack-plugin": "^2.0.1",
    "prettier": "^2.6.2",
    "sass": "^1.58.0",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^3.0.1",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.3.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,svelte,json,md}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,vue,svelte}": [
      "stylelint --fix"
    ]
  }
}
